#include <stdio.h>
#include <cs50.h>

int main(void)
{
    int months;
    months = get_int("Enter number: ");

    if (months > 0)
    {
        if ((months == 1) || (months == 2) || (months == 12))
        {
        printf("Winter\n");
        }
        if ((months == 3) || (months == 4) || (months == 5))
        {
        printf("Spring\n");
        }
        if ((months == 6) || (months == 7) || (months == 8))
        {
        printf("Summer\n");
        }
        if ((months == 9) || (months == 10) || (months == 11))
        {
        printf("Autumn\n");
        }
    }
    else
    {
        printf("Enter number from 1 to 12\n");
    }
}
